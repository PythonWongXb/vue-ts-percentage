{
  "name": "hello-world-vue",
  "displayName": "hello-world-vue",
  "description": "A hello world sample extension to demonstrate using Vue with the Webview UI Toolkit for VS Code.",
  "version": "0.0.2",
  "publisher": "EvanZuo",
  "engines": {
    "vscode": "^1.77.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vuetspercentage2.helloWorld",
        "title": "vuetspercentage2"
      },
      {
        "command": "hello-world.showHelloWorld",
        "title": "Hello World (Vue): Show"
      }
    ]
  },
  "scripts": {
    "install:all": "npm install && npm --prefix ./webview-ui install ./webview-ui",
    "dev:webview": "cd webview-ui && pnpm run dev",
    "build:webview": "cd webview-ui && pnpm run build",
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.46.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.0"
  }
}
